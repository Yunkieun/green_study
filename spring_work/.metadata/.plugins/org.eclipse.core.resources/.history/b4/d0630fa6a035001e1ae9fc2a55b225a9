package di_p;

public class Tire {

	String name;
	int num;
	
	public Tire() {
		System.out.println("Tire 기본생성자");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		System.out.println("Tire setName():"+name);
		this.name = name;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		System.out.println("Tire.setNum():"+num);
		this.num = num;
	}
	
	
}
